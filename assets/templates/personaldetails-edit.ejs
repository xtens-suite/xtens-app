<div class="row text-center">
  <h3><%= __('personal-details')%></h3>
</div>
<% if( !data.id) {%>
<div class="row text-center">
  <i style="margin-right:1em;" class="fa fa-info-circle"></i></i><%= __('personal-details-info-message')%>
</div>
<%}%>
<div class="form-group"></div>
<div class="form-group metadataform-group">
    <label for="givenName" class="data-label"><%= __('given-name') %></label>
    <div class="data-input-div">
        <input text class="form-control personaldetails-input" id="givenName" name="givenName"></input>
    </div>
</div>
<div class="form-group metadataform-group">
    <label for="surname" class="data-label"><%= __('surname') %></label>
    <div class="data-input-div">
        <input text class="form-control personaldetails-input" id="surname" name="surname"></input>
    </div>
</div>
<div class="form-group metadataform-group">
    <label for="birth-date" class="data-label"><%= __('birth-date') %></label>
    <div class="data-input-div">
        <input text class="form-control personaldetails-input" id="birthDate" name="birthDate" placeholder="<%= __('please-select') %>"></input>
    </div>
</div>
